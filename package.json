{
  "name": "exile-watch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "client/node_modules/.bin/next build && next export",
    "start": "NODE_ENV=production node server.js",
    "type-check": "client/node_modules/.bin/tsc",
    "svgr:icons": "npx @svgr/cli --svgo-config ../writ/packages/writ-icons/.svgo.json -d design-system/icons design-system/icons/svg --typescript --memo --ref",
    "svgr:trees": "npx @svgr/cli --svgo-config .svgo.trees.json -d features/builds/components/SkillTree/3.14/components features/builds/components/SkillTree/3.14/svg --typescript --memo",
    "lint": "yarn client/node_modules/.bin/eslint --fix",
    "generate:builds:skill-tree-svg": "client/node_modules/.bin/ts-node-transpile-only features/builds/scripts/generate-skill-tree.ts",
    "generate:builds:ascendencies-tree-svg": "client/node_modules/.bin/ts-node-transpile-only features/builds/scripts/generate-skill-tree.ts",
    "generate:builds:trees": "client/node_modules/.bin/ts-node-transpile-only features/builds/scripts/generate-skill-tree.ts && ts-node-transpile-only features/builds/scripts/generate-ascendency-trees.ts && yarn svgr:trees",
    "generate:builds": "yarn generate:builds:trees",
    "generate:sitemap": "node build-tools/generate/generateSitemap.js",
    "generate:all": "yarn generate:encounters && yarn generate:builds",
    "extract:tokens:gems": "client/node_modules/.bin/ts-node features/builds/scripts/extract-gems.ts",
    "extract:builds": "yarn extract:tokens:map-mods",
    "commit": "client/node_modules/.bin/cz",
    "storybook": "client/node_modules/.bin/start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "client/node_modules/.bin/build-storybook"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@exile-watch/encounter-data": "^0.2.0",
    "@exile-watch/postcss-config": "^0.1.0",
    "@exile-watch/writ-icons": "^0.3.5",
    "@exile-watch/writ-react": "^0.2.5",
    "@mantine/core": "^7.5.2",
    "@mantine/hooks": "^7.5.2",
    "@sentry/browser": "^6.4.1",
    "@sentry/integrations": "^6.4.1",
    "@sentry/node": "^6.4.1",
    "@sentry/react": "^6.4.1",
    "@sentry/tracing": "^6.4.1",
    "@sentry/webpack-plugin": "^1.15.1",
    "@tabler/icons-react": "^2.47.0",
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^18.2.0",
    "react-xml-parser": "^1.1.8",
    "sass": "^1.34.0",
    "xml2ts": "^0.6.1",
    "xmltojson": "^1.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@exile-watch/typescript-config": "^0.1.19",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.17.0",
    "@types/react": "^18.2.52",
    "@types/xmltojson": "^0.0.28",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "css-loader": "^5.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.21.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sass-loader": "^10",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1"
  }
}
