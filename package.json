    "svgr": "npx @svgr/cli -d assets/icons assets/icons/svg --typescript --memo",
    "lint": "yarn eslint --fix",
    "build:encounters:indexed-search": "node features/encounters/scripts/build-indexed-search.js",
    "build:encounters:paths": "node features/encounters/scripts/build-paths.js",
    "extract:encounters:tokens": "node features/encounters/scripts/extract-tokens.js",
    "extract:spells:tokens": "node features/encounters/scripts/extract-skill-tokens.js",
    "clean-invalid-data-files": "node features/encounters/scripts/clean-invalid-data-files.js",
    "data:encounters:update": "yarn extract:spells:tokens && yarn extract:encounters:tokens && yarn build:encounters:indexed-search && yarn build:encounters:paths"
  },
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.21",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.8"
